[alias]
	fakemerge = merge --no-ff --no-commit --strategy ours
	cpx = cherry-pick -x
	mt = mergetool
	lp = log -p
	s = status
	ss = show
	l1 = log --oneline
	cpc = cherry-pick --continue
	c1 = clean -i
	l = log --oneline --graph --all
	ds = diff --staged
	ldate = log --graph --pretty=format:'%C(yellow)%h %Cred%ad %Cblue%an%Cgreen%d %Creset%s'
	zip = archive --format zip --output Archive.zip HEAD
[color]
	ui = auto
[commit]
	gpgSign = true 
[core]
	excludesfile = ~/.gitignore
	pager = delta
[delta]
	navigate = true
[diff]
	renameLimit = 999999
[http]
	cookiefile = /home/home/.gitcookies
[interactive]
	diffFilter = delta --color-only
[merge]
	tool = meld
	renameLimit = 999999
[rerere]
	enabled = false
[url "https://invent.kde.org/"]
	insteadOf = kde:
[url "ssh://git@invent.kde.org/"]
	pushInsteadOf = kde:
[user]
	name = Daniel Tang
	email = danielzgtg.opensource@gmail.com
